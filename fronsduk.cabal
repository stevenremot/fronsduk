category:            Development
build-type:          Simple
cabal-version:       >=1.8

executable fronsduk
  hs-source-dirs:      src
  main-is: Interpreter.hs
  ghc-options: -main-is Interpreter
  build-depends:
    base ==4.6.*,
    binary,
    bytestring

executable fronsduk-assemble
  hs-source-dirs: src
  main-is: Assembler.hs
  ghc-options: -main-is Assembler
  build-depends:
    base == 4.6.*,
    parsec,
    binary,
    bytestring

executable fronsduk-disassemble
  hs-source-dirs: src
  main-is: Disassembler.hs
  ghc-options: -main-is Disassembler
  build-depends:
    base == 4.6.*,
    binary,
    bytestring
